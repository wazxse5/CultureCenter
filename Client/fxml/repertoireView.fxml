<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.controller.RepertuarViewController">
   <children>
      <Label layoutX="257.0" layoutY="6.0" prefHeight="0.0" prefWidth="109.0" text="Repertuar">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <TableView fx:id="tableView" layoutX="16.0" layoutY="54.0" prefHeight="209.0" prefWidth="570.0">
         <columns>
            <TableColumn fx:id="columnIdEvent" prefWidth="38.0" text="idEvent" visible="false" />
            <TableColumn fx:id="columnName" minWidth="100.0" prefWidth="350.0" text="Nazwa" />
            <TableColumn fx:id="columnDate" minWidth="60.0" prefWidth="120.0" text="Data" />
            <TableColumn fx:id="columnStartTime" minWidth="50.0" prefWidth="98.0" text="Godzina" />
            <TableColumn fx:id="columnIdEventType" prefWidth="38.0" text="idEventType" visible="false" />
         </columns>
      </TableView>
      <HBox layoutX="69.0" layoutY="301.0" prefHeight="49.0" prefWidth="404.0">
         <children>
            <Button fx:id="ChooseButton" mnemonicParsing="false" onAction="#choose" prefHeight="24.0" prefWidth="82.0" text="Wybierz">
               <font>
                  <Font size="11.0" />
               </font>
               <HBox.margin>
                  <Insets left="10.0" right="10.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="refreshButton" mnemonicParsing="false" onAction="#refresh" prefHeight="25.0" prefWidth="100.0" text="Odśwież">
               <HBox.margin>
                  <Insets left="10.0" right="10.0" />
               </HBox.margin>
               <font>
                  <Font size="11.0" />
               </font>
            </Button>
         </children>
      </HBox>
   </children>
</AnchorPane>
