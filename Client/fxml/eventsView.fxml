<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.controller.EventsViewController">
   <children>
      <Label alignment="CENTER" layoutX="175.0" layoutY="29.0" prefHeight="30.0" prefWidth="251.0" text="Wydarzenia">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <Button fx:id="backToLoginButton" layoutX="334.0" layoutY="321.0" mnemonicParsing="false" onAction="#back" prefHeight="40.0" prefWidth="240.0" text="Cofnij">
         <font>
            <Font size="16.0" />
         </font>
      </Button>
      <Button fx:id="ReserveButton" layoutX="38.0" layoutY="309.0" mnemonicParsing="false" onAction="#reserve" prefHeight="24.0" prefWidth="82.0" text="Zarezerwuj">
         <font>
            <Font size="11.0" />
         </font>
      </Button>
      <Button fx:id="BuyButton" layoutX="147.0" layoutY="309.0" mnemonicParsing="false" onAction="#buy" prefHeight="24.0" prefWidth="82.0" text="Kup">
         <font>
            <Font size="11.0" />
         </font>
      </Button>
      <TableView fx:id="tableView" layoutX="17.0" layoutY="68.0" prefHeight="209.0" prefWidth="568.0">
        <columns>
          <TableColumn fx:id="columnId" prefWidth="122.0" text="Id wydarzenia" visible="false" />
